import seaborn as sns
from sklearn.datasets import make_blobs  #to generate clustered data


X, y = make_blobs(
    n_samples=1000,
    n_features=2,
    centers=4,
    random_state=42
)


# Visualize 
sns.scatterplot(x=X[:,0], y=X[:,1])


# K-means algo
from sklearn.cluster import KMeans


K = 4

kmeans = KMeans(
    n_clusters=K,
    init='k-means++',
    random_state=42
)


labels = kmeans.fit_predict(X)
#label = cluster numbers



