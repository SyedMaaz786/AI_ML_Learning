import numpy as np
import time


#Execution performance
size = 100_000_000

#Python list
py_list = list(range(size))
start = time.time()
sq_arr = [i**2 for i in py_list]
end = time.time()
print(f"Python list time = {end-start} seconds")

#Numpy array
np_arr = np.array(py_list)
start = time.time()
sq_arr = np_arr ** 2
end = time.time()
print(f"Numpy array time = {end-start} seconds")



